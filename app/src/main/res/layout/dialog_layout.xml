<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:text="Neutral"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/neutralButton" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:text="Handicap"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/handicapButton" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:text="Family"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/familyButton" />

    <ImageButton
        android:id="@+id/handicapButton"
        android:layout_width="75dp"
        android:layout_height="64dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="108dp"
        android:background="#00F1ECEC"
        android:scaleType="fitXY"
        android:src="@drawable/handicap_on"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/neutralButton"
        app:layout_constraintTop_toBottomOf="@+id/womenButton" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="P r e f e r e n c e s"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/menButton"
        android:layout_width="75dp"
        android:layout_height="64dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/male_on"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/neutralButton"
        android:layout_width="75dp"
        android:layout_height="64dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="44dp"
        android:background="#00EBE8E8"
        android:scaleType="fitXY"
        android:src="@drawable/neutral_on"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menButton" />

    <ImageButton
        android:id="@+id/womenButton"
        android:layout_width="75dp"
        android:layout_height="64dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="24dp"
        android:background="#00F0EBEB"
        android:scaleType="fitXY"
        android:src="@drawable/female_on"
        app:layout_constraintStart_toEndOf="@+id/menButton"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/familyButton"
        android:layout_width="75dp"
        android:layout_height="64dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="24dp"
        android:background="#00FDF8F8"
        android:scaleType="fitXY"
        android:src="@drawable/family_on"
        app:layout_constraintStart_toEndOf="@+id/womenButton"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/button19"
        android:layout_width="290dp"
        android:layout_height="38dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/button_round"
        android:text="Reset"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/handicapButton" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Map"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button19" />

    <Switch
        android:id="@+id/mapSwitch"
        android:layout_width="307dp"
        android:layout_height="27dp"
        android:layout_marginTop="12dp"
        android:text="Satellite / Normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:layout_width="196dp"
        android:layout_height="62dp"
        android:layout_marginTop="44dp"
        android:text="DEMO"
        android:textColor="#00FCFBFB"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mapSwitch" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:text="Men"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menButton" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="94dp"
        android:text="Women"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/womenButton" />

</androidx.constraintlayout.widget.ConstraintLayout>